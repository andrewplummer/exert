{
  "name": "exert",
  "version": "0.0.2",
  "description": "A small library to create watchable/observable classes with decorators",
  "main": "lib/exert.js",
  "scripts": {
    "build": "webpack --env dev && webpack --env build && npm run test",
    "dev": "webpack --progress --colors --watch --env dev",
    "test": "mocha --require babel-core/register --colors ./test/*.spec.js",
    "test:watch": "mocha --require babel-core/register --colors -w ./test/*.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/uxder/exert.git"
  },
  "keywords": [
    "observable",
    "es6",
    "streams"
  ],
  "author": "Scott Murphy",
  "license": "MIT",
  "homepage": "https://github.com/uxder/exert.git",
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "8.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-transform-decorators": "6.24.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "chai": "4.1.2",
    "eslint": "4.13.1",
    "eslint-loader": "1.9.0",
    "mocha": "4.0.1",
    "webpack": "3.10.0",
    "yargs": "10.0.3"
  }
}
